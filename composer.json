{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "https://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "yiisoft/yii2-bootstrap": "2.0.11",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-imagine": "^2.3",
        "kartik-v/yii2-widget-sidenav": "^1.0",
        "voskobovich/yii2-linker-behavior": "^4.1",
        "2amigos/yii2-tinymce-widget": "^1.1",
        "kartik-v/yii2-mpdf": "^1.0",
        "2amigos/qrcode-library": "^2.0",
        "picqer/php-barcode-generator": "^2.2",
        "kartik-v/yii2-widget-fileinput": "^1.1",
        "kartik-v/yii2-widget-select2": "^2.2",
        "linslin/yii2-curl": "^1.5",
        "himiklab/yii2-recaptcha-widget": "^2.1",
        "vova07/yii2-fileapi-widget": "^0.1.9",
        "2amigos/yii2-file-upload-widget": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "longman/telegram-bot": "^0.75.0",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0",
        "ext-simplexml": "*",
        "kartik-v/yii2-tree-manager": "@dev",
        "vova07/yii2-imperavi-widget": "2.0.11",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-editable": "dev-master",
        "nick-denry/yii2-round-switch-column": "^0.1.2",
        "yiisoft/yii2": "^2.0",
        "kartik-v/yii2-bootstrap5-dropdown": "@dev",
        "kartik-v/yii2-export": "@dev",
        "palax/yii2-core": "0.5.3",
        "palax/core": "0.1.6",
        "zircote/swagger-php": "^4.2"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
